{
  "name": "vtex-checkout",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run vite-build:watch:dev\" \"npm run preview\"",
    "prod": "cross-env NODE_ENV=production concurrently \"npm run vite-build:watch:prod\" \"npm run preview\"",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "vite-build:watch:dev": "cross-env NODE_ENV=development vite build --mode development --watch",
    "vite-build:watch:prod": "cross-env NODE_ENV=production vite build --watch",
    "preview": "vite preview"
  },
  "dependencies": {
    "jquery": "^3.6.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.2",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.16",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.1",
    "terser": "^5.16.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-externals": "^0.5.1"
  }
}
